{
    "name": "名前",
    "manifest_version": 2,
    "version": "1.0",
    "browser_action": {
        "default_icon": "icon_32.png",
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content_scripts.js"
            ]
        }
    ],
    "background": {
        "scripts": [
            "background.js"
        ]
    }
}